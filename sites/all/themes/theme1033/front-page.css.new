.views-row.category-row{
    margin-bottom: 60px;
}
.view-categories .jcarousel-skin-default .jcarousel-item {
    width: 198px !important;
    height: 200px;
    display: inline-block;
    float: none !important;
}
.view-categories .jcarousel-skin-default .jcarousel-next-horizontal {
    position: absolute;
    top: 48%;
    right: 40px;
    background-position: 0 -32px;
}
.view-categories .jcarousel-skin-default .jcarousel-prev-horizontal {
    position: absolute;
    top: 48%;
    left: 40px;
    background-position: 0 0;
}
.category-row {
    width: 44%;
    background-color: #fff;
    padding:5px 1%;
    border-radius: 20px;
    min-height: 130px;
    float: left;
}
.cat-pick{
    display: flex;
    flex-wrap: wrap;
    align-items: center;
    position: relative;
    height: 220px;
    background-color: #e6e6e6;
}
/*.cat-pick:hover .category-desc {*/
