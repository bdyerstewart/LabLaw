/*}*/
.category-image{
    float: left;
    padding: 0;
    width: 100%;
}
.category-image img {
    filter: grayscale(1);
}
.category-title {
    font-size: 20px;
    font-weight: bold;
    line-height: 1.2;
    text-align: center;
    color: #fff;
    width: 100%;
    padding: 0 20px;
    opacity: 1;
    z-index: 10;
    text-shadow: 2px 2px #000;
}
.category-desc {
    font-size: 85%;
    line-height: 1.2;
    text-align: center;
    padding: 0 15px;
    display: none;
}

.kl-img {
    width: 99%;
    -webkit-border-radius: 75px;
    -moz-border-radius: 75px;
    border-radius: 100px;
    height: auto;
    position: absolute;
    margin-right: 20px;
    overflow: hidden;
    top: 0;
    border: 1px solid transparent;
}
.kl-img img {
    /*opacity:0.35;*/
    border-radius: 100px;
}
.kl-img::before {
    padding: 50% 0;
    content: "";
    float: left;
}
