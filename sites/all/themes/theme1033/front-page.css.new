.front .portfolio-row {
    width: 95%;
    font-size: 1.3em;
    background-color: #fff;
    min-height: 325px;
    position: relative;
    padding: 15px 1% 0;
    margin: 0 1.2% 0 1.8%;
    float: left;
    border: 1px solid #fff;
    color: #000;
}
.front .portfolio-row a{
    color: #000;
}
.front .portfolio-row:hover{
    border: 1px solid #000;
    background-color: #eee;

}
.front #footer .portfolio-row:hover  a{
    color: #000;
}
.front .region-footer {
    padding: 0;
}
